#Cleans out Addigy cached items

if [ -d "/Library/Addigy/ansible/packages" ]; then
    rm -Rf /Library/Addigy/ansible/packages/*; then
    echo "Addigy Library Cache Cleared"
fi

if [ -d "/Library/Addigy/download-cache/downloaded" ]; then
    rm -Rf /Library/Addigy/download-cache/downloaded/*; then
    echo "Addigy Download Cache Cleared"
fi